package HomeWork;

// ** static, instance 비교
// => 교재 174 ~ 175, 188 ~ 190 (예습)
// => 숙제: 207P 6-1

public class homework_20230904 {

	public static void main(String[] args) {
		// 6-1) 다음과 같은 멤버변수를 갖는 Student 클래스를 정의하시오.
		// 타입        변수명        설명
		// String     name        학생이름
		// int        ban         반
		// int        no          번호
		// int        kor         국어점수
		// int        eng         영어점수
		// int        math        수학점수

		class Student {
			String name;
			int ban;
			int no;
			int kor;
			int eng;
			int math;
		}
		
	} //main

} //class
