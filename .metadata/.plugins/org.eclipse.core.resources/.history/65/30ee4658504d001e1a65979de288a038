package HomeWork;

public class Animal {
	String breath;
	String leg;
	
	public Animal(String breath, String leg) {
		this.breath = breath;
		this.leg = leg;
	}
	
	public void breath() {
		System.out.println("breath => " + breath);
	}
	
	public void leg() {
		System.out.println("leg => " + breath);
	}
	 
}
	

