package HomeWork;

public class Animal {
	String breath;
	int leg;
	
	public Animal(String breath, int leg) {
		this.breath = breath;
		this.leg = leg;
	}
	
	public void breath() {
		System.out.println("breath => " + breath);
	}
	
	public void leg() {
		System.out.println("leg => " + breath);
	}
	
	public static void main(String[] args) {
		Animal lion = new Animal("Ìù•!", 4);
		lion.breath();
	}
	 
}

