package j07_classExtends;

public class Ex01_Student {
	// => 변수
	private String no;
	private String name;
	private int score;
	
	Ex01_Car car = new Ex01_Car(1000, 5000, "Blue");
	
	// 생성자
	Ex01_Student() {}
	Ex01_Student(String no, String name, int score) {
		this.no = no;
		this.name = name;
		this.score = score;
	}
	
	// => 생성시에 초기화 하고 readonly 로 사용
	
	
} // class
