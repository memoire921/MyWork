package HomeWork;

public class Animal {
	String breath;
	String leg;
	
	public Animal(String breath, String leg) {
		this.breath = breath;
		this.leg = leg;
	}
	
	public void breath() {
		System.out.println("breath => " + breath);
	}
	
	
	
	
	
	
	
	public leg() {
		System.out.println("동물은 다리가 있다");
	}
	 
}
	

class Mammal extends Animal {
	
	String cring;
	String run;
}

class Wild extends Mammal {
	String lion;
	String tiger;
}

